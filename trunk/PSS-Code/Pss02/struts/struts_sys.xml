<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC   
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"  
    "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
	<package name="sys" namespace="/sys" extends="abs">
		<action name="*_*" class="com.pss.web.action.system.{1}Action"
			method="{2}">
			<result>/ftl/sys/{1}/{2}.html</result>
		</action>
	</package>

	<package name="json" namespace="/sys" extends="baseJson">
		<action name="*/*" class="com.pss.web.action.system.{1}Action"
			method="{2}">
			<result />
		</action>
	</package>
</struts>
